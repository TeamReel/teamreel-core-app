[run]
source = src
omit = 
    src/git_reporter.py,
    src/github_reporter.py,
    src/plan_validator.py,
    src/spec_validator.py,
    src/task_validator.py,
    src/complexity_analyzer.py,
    src/coverage_validator.py,
    src/naming_validator.py,
    src/security_scanner.py,
    src/plugins/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov